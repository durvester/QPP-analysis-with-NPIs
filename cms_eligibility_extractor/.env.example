# Environment Configuration

# Input file path
NPI_CSV_PATH=../NPI.csv

# Output directory paths
OUTPUT_BASE_DIR=./outputs
CSV_OUTPUT_DIR=./outputs/csv
DATABASE_OUTPUT_DIR=./outputs/database
EXCEL_OUTPUT_DIR=./outputs/excel
RAW_OUTPUT_DIR=./outputs/raw

# Logging configuration
LOG_LEVEL=INFO
LOG_FILE=./logs/extraction.log

# Processing configuration
BATCH_SIZE=100
MAX_CONCURRENT_REQUESTS=5
ENABLE_PROGRESS_BAR=true

# Output format options
GENERATE_CSV=true
GENERATE_SQLITE=true
GENERATE_EXCEL=true
SAVE_RAW_RESPONSES=true

# CSV export strategy (normalized or tiered)
CSV_STRATEGY=normalized

# Resume processing from last checkpoint
RESUME_FROM_CHECKPOINT=true
CHECKPOINT_INTERVAL=1000